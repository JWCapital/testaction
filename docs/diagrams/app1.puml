@startuml
class Person {
  - name: String
  - age: int
  + Person(name: String, age: int)
  + getName(): String
  + getAge(): int
  + setName(name: String): void
  + setAge(age: int): void
}

class Address {
  - street: String
  - city: String
  - zipCode: String
  + Address(street: String, city: String, zipCode: String)
  + getStreet(): String
  + getCity(): String
  + getZipCode(): String
  + setStreet(street: String): void
  + setCity(city: String): void
  + setZipCode(zipCode: String): void
}

Person -- Address : has

@enduml
